{
  "$schema": "https://openapis.vercel.sh/vercel.json",
  "buildCommand": "cd ambient-simulation && npm install && npm run build",
  "outputDirectory": "ambient-simulation/.next",
  "devCommand": "cd ambient-simulation && npm run dev",
  "installCommand": "cd ambient-simulation && npm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "env": {
    "NEXT_PUBLIC_AMBIENT_API_KEY": "@ambient_api_key"
  }
}
